    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "auth_client",
            "cwd": "parqueadero_movil\\packages\\auth_client",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "my_app",
            "cwd": "parqueadero_movil\\parqueadero_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_production.dart",
            "args": [
                "--flavor",
                "production"
            ]
        },
        {
            "name": "my_app (profile mode)",
            "cwd": "parqueadero_movil\\parqueadero_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_development.dart",
            "args": [
                "--flavor",
                "development"
            ]
        },
        {
            "name": "my_app (release mode)",
            "cwd": "parqueadero_movil\\parqueadero_app",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main_development.dart",
            "args": [
                "--flavor",
                "development"
            ]
        }
    ]
}